{
  "name": "sanity-shopify-autofill",
  "type": "commonjs",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Dan Courtness",
  "description": "Converts products from a Shopify catalogue into Sanity documents.",
  "keywords": [
    "sanity",
    "shopify",
    "import"
  ],
  "main": "index.js",
  "scripts": {
    "build": "npx babel src --out-dir dist --presets=@babel/preset-env",
    "start": "npx babel src --watch --out-dir dist --presets=@babel/preset-env",
    "test": "echo \"Error: no test specified\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  },
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "./",
        "rootPathPrefix": "~/"
      }
    ]
  ]
